<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.g52grp.views.JobMenuController">
   <children>
      <Button fx:id="addNewJob" layoutX="567.0" layoutY="637.0" mnemonicParsing="false" onAction="#newJobModal" text="Add New Job">
         <font>
            <Font name="AppleGothic Regular" size="14.0" />
         </font></Button>
      <Label layoutX="552.0" layoutY="49.0" text="Current Jobs">
         <font>
            <Font name="AppleGothic Regular" size="24.0" />
         </font>
      </Label>
      <TableView fx:id="jobTable" layoutX="218.0" layoutY="100.0" prefHeight="500.0" prefWidth="800.0">
        <columns>
          <TableColumn fx:id="siteName" prefWidth="124.0" text="Site Name" />
          <TableColumn fx:id="plotNumber" prefWidth="122.0" text="Plot Number" />
            <TableColumn fx:id="date" prefWidth="124.0" text="Date" />
            <TableColumn fx:id="jobId" maxWidth="0.0" minWidth="0.0" prefWidth="0.0" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <ImageView fitHeight="50.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../resources/rjb.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
